{
  "name": "animation-studio",
  "version": "1.0.0",
  "description": "A powerful, web-based animation tool built from scratch using pure HTML, CSS, and JavaScript. No third-party animation platforms required - complete control over every aspect of your animations.",
  "main": "index.html",
  "scripts": {
    "dev": "vite --port 3000 --open",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "http-server . -p 8080 -o",
    "start": "npm run dev",
    "setup": "node scripts/dev-setup.js",
    "lint": "eslint js/**/*.js --fix",
    "format": "prettier --write .",
    "test": "echo \"No tests specified\" && exit 0",
    "clean": "rimraf dist",
    "deploy": "npm run build && npm run preview",
    "postinstall": "echo \"âœ… Installation complete! Run 'npm run dev' to start developing.\""
  },
  "keywords": [
    "animation",
    "web-app",
    "canvas",
    "timeline",
    "keyframes",
    "video-export",
    "2d-animation",
    "toonly-alternative",
    "javascript",
    "html5-canvas"
  ],
  "author": "AnimationStudio",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/animation-studio.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/animation-studio/issues"
  },
  "homepage": "https://github.com/yourusername/animation-studio#readme",
  "devDependencies": {
    "vite": "^5.0.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "prettier": "^3.1.0",
    "http-server": "^14.1.1",
    "rimraf": "^5.0.5",
    "@vitejs/plugin-legacy": "^5.2.0",
    "terser": "^5.24.0"
  },
  "dependencies": {
    "gif.js": "^0.2.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "type": "module",
  "files": [
    "index.html",
    "js/",
    "styles/",
    "assets/",
    "README.md",
    "LICENSE"
  ]
}